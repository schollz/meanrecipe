<script>

var recipe_engine = new Bloodhound({
  datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
  queryTokenizer: Bloodhound.tokenizers.whitespace,
  remote: {
      url: '/suggest/recipe.json?q=%QUERY',
      wildcard: '%QUERY',
      rateLimitWait : 500,
      filter: function (recipes) {
      if( recipes && recipes.error === "AccessDenied" ){
        window.location = '/login';
      }else{
          return $.map(recipes, function (recipe) {
               return {
                  value: recipe.title, link: recipe.link
                };
            });
        }
      }
  }
});

var post_engine = new Bloodhound({
  datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
  queryTokenizer: Bloodhound.tokenizers.whitespace,
  remote: {
      url: '/suggest/post.json?q=%QUERY',
      wildcard: '%QUERY',
      rateLimitWait : 500,
      filter: function (posts) {
      if( posts && posts.error === "AccessDenied" ){
        window.location = '/login';
      }else{
          return $.map(posts, function (post) {
               return {
                  value: post.title, link: post.link
                };
            });
        }
      }
  }
});

var menu_engine = new Bloodhound({
  datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
  queryTokenizer: Bloodhound.tokenizers.whitespace,
  remote: {
      url: '/suggest/menu.json?q=%QUERY',
      wildcard: '%QUERY',
      rateLimitWait : 500,
      filter: function (menus) {
      if( menus && menus.error === "AccessDenied" ){
        window.location = '/login';
      }else{

          return $.map(menus, function (menu) {
               return {
                  value: menu.title, link: menu.link
                };
            });
        }
      }
  }
});



recipe_engine.clear();
recipe_engine.initialize();


post_engine.clear();
post_engine.initialize();

menu_engine.clear();
menu_engine.initialize();

$modal_search_input = $(".modal--search__form").find('input[type=text].embed-submit__textfield');

$modal_search_input.typeahead('destroy');

$modal_search_input.typeahead(
    {
        highlight: true,
        hint : false,
      },
      {
        name: 'recipe',
        limit: 20,
        displayKey: 'value',
        source: recipe_engine.ttAdapter(),
        templates: {
              header: '<h3 class="group-header">Recipes</h3>',
          //empty: '<p class="empty">Nothing found</p>'
        }
    },
    {
          name: 'post',
          limit : 20,
          displayKey: 'value',
          source: post_engine.ttAdapter(),
          templates: {
             header: '<h3 class="group-header">Articles</h3>',
        //empty: '<p class="empty">Nothing found</p>'
          }
      },
      {
            name: 'menu',
            limit : 20,
            displayKey: 'value',
            source: menu_engine.ttAdapter(),
            templates: {
              // header: '<h3 class="group-header">Menus</h3>',
          // empty: '<p class="empty">Nothing found</p>',
            }
      }

  ).on('typeahead:selected', function (obj, datum,src ) {

     window.location = datum.link;

  }).on('typeahead:closed', function (obj, datum, name) {

	    $(obj.currentTarget).val("");

  }).on('keyup', this, function (event) {

      if (event.keyCode == 13) {
    	  $modal_search_input.typeahead('close');
      }

  });
</script>

		

        }

        function showMore() {
            getShowMore().hide();
            getInfoItems().show();
            getShowLess().show();
        }

        function showLess() {
            getShowLess().hide();
            getShowMore().show();
        }

        function goToRequestBooklet() {
            location.href = "/booklet-request";
